{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "doctemplate",
      "camelcase": "Doctemplate",
      "title": "Doctemplate",
      "scope": "source.doctemplate",
      "file-types": [
        "doctemplate"
      ],
      "injection-regex": "^doctemplate$",
      "class-name": "TreeSitterDoctemplate"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter grammar for Pandoc doctemplate files",
    "authors": [
      {
        "name": "Posit, PBC",
        "email": "carlos.scheidegger@posit.co"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-doctemplate"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}