{
  "name": "@quarto/quarto-sync-client",
  "version": "0.0.1",
  "private": true,
  "description": "Automerge sync client for Quarto collaborative documents",
  "license": "MIT",
  "author": {
    "name": "Posit PBC"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
