    "test:watch": "vitest"
  },
  "dependencies": {
    "@automerge/automerge-repo": "^2.5.1",
    "@automerge/automerge-repo-network-websocket": "^2.5.1",
    "@quarto/quarto-automerge-schema": "*",
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vitest": "^4.0.17"
  }
}
